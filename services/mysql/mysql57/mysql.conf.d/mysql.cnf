[client]
port		= 3306

[mysqld]
port		                    = 3306

sql_mode=NO_AUTO_CREATE_USER
explicit_defaults_for_timestamp = true
lower_case_table_names  = 2

slow_query_log          = 1
long_query_time         = 3
slow-query-log-file     = /var/log/mysql/mysql.slow.log
log-error               = /var/log/mysql/mysql.error.log